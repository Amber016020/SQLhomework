@model List<DBClassLibrary.Models.User>
@{
    ViewData["Title"] = "Register";
}

<h2>註冊</h2>

使用者名稱
<p>
    <input type="text" name=" username" size="15" maxlength="30" />
</p>
密碼
<p>
    <input type="password" name=" password" size="15" maxlength="30" />
</p>
性別：
<p>
    男<input type="radio" name=" sex" value="man" />
    女<input type="radio" name=" sex" value="female" />
</p>
生日
<p>
    <input type="date" name="data" />
</p>
email
<p>
    <input type="email" name=" email" size="30" maxlength="30" />
</p>
連絡電話
<p>
    <input type="tel" name=" tel" size="30" maxlength="30" />
</p>
地址
<p>
    <textarea name="address" cols="30" rows="4">輸入您的地址...</textarea>
</p>
<input type="submit" value="submit" />